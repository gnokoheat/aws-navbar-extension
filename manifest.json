{
  "name": "AWS Navbar Region Color & Flag",
  "description": "Change navbar color and flag according to AWS region",
  "version": "1.1.0",
  "manifest_version": 3,
  "icons": {
    "128": "icon.png"
  },
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://*.console.aws.amazon.com/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "*://*.console.aws.amazon.com/*"
      ],
      "js": [
        "main.js"
      ]
    }
  ]
}
